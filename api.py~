import urllib2
import json

url="http://api.rottentomatoes.com/api/public/v1.0/movies.json?apikey=au5nf6pd9fwwh4kjbqqt6jws&q=%s"
key = "au5nf6pd9fwwh4kjbqqt6jws"

def getStuff(query):
    request = urllib2.urlopen(url % query)
    result = request.read()
    d = json.loads(result)
    rlist = d['movies']
    for r in rlist:
        print r['titleNoFormatting']
    



